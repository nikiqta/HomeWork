<h1>Teams</h1>
<form class="search">
    <label>Seach: </label>
    <input type="text">
    <input type="submit" value="Search">
</form>
<div class="teams">
    {{#each teams}}
        <div class="team">
            <h2>{{name}}</h2>
            <h3>Members</h3>
            {{#each members}}
            <div class="team-info">
            <p>{{firstName}} {{lastName}}</p>
            {{else}}
                <p>Team has no members</p>
            </div>
            {{/each}}
            <div class="team-info">
                <h3>Projects</h3>
                {{#each projects}}
                <p>{{name}}</p>
                {{else}}
                    <p>Team has no projects</p>
                {{/each}}
            </div>
        </div>
    {{/each}}
</div>